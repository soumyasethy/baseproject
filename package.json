{
  "name": "BaseProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "code-push debug android": "code-push debug android",
    "android": "react-native run-android",
    "android-release": "react-native run-android --variant=release",
    "log": "react-native log-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "podInstall": "cd ios && pod install && cd ..",
    "create-release-bundle-android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "create-bundle-android": "react-native bundle --platform android --dev false --entry-file index.ts --bundle-output output/index.android.bundle --assets-dest output/",
    "create-bundle-ios": "react-native bundle --platform ios --dev false --entry-file index.ts --bundle-output output/index.ios.bundle --assets-dest output/",
    "build:ios": "react-native bundle --entry-file='index.ios.js' --bundle-output='./ios/BaseProject/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "gzip-android": "gzip ./output/index.android.bundle",
    "gzip-ios": "gzip ./output/index.ios.bundle",
    "android-tcpip": "adb tcpip 5555",
    "android-reverse-tcp": "adb reverse tcp:8081 tcp:8081",
    "appcenter-code-push-stagging-android": "appcenter codepush release-react DataCollector-AndroidRN android",
    "code-push-stagging-android": "code-push release-react DataCollector-AndroidRN android",
    "code-push-stagging-ios": "code-push release-react DataCollector ios",
    "code-push deployment ls MyApp-ios -k": "code-push deployment ls DataCollector-AndroidRN -k",
    "npx-jetify": "npx jetify",
    "codepush-deployment-list": "appcenter codepush deployment list -a soumyasethy/DataCollector-AndroidRN",
    "mandatory-update": "appcenter codepush release-react -a soumyasethy/DataCollector-AndroidRN  -m --description \"Modified the header color\"",
    "react-native run-ios --configuration Release": "react-native run-ios --configuration Release"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/cli-platform-android": "3.0.3",
    "@react-native-community/masked-view": "^0.1.6",
    "@react-native-community/netinfo": "^5.3.3",
    "@react-navigation/native": "^5.0.0-alpha.33",
    "@react-navigation/stack": "^5.0.0-alpha.68",
    "axios": "^0.19.2",
    "component-library": "git+https://github.com/soumyasethy/component-library#production",
    "mobx": "^5.14.2",
    "mobx-react": "^6.1.4",
    "moment": "^2.24.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-code-push": "^6.1.0",
    "react-native-geolocation-service": "^4.0.0",
    "react-native-gesture-handler": "^1.5.5",
    "react-native-image-picker": "^2.3.0",
    "react-native-maps": "0.26.1",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.4",
    "react-native-screens": "^2.0.0-alpha.33",
    "react-native-snackbar": "^2.2.0",
    "react-native-svg": "^11.0.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-swiper": "^1.6.0-nightly.5"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
